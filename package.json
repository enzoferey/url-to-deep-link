{
  "name": "url-to-deep-link",
  "version": "1.0.7",
  "description": "Transform URLs to Deep Links and open your users apps",
  "main": "./dist/index.js",
  "files": [
    "dist",
    "*.md"
  ],
  "scripts": {
    "build": "webpack --mode=\"production\"",
    "watch": "webpack --watch",
    "prettier": "prettier --config \"./.prettierrc.js\" --write \"./lib/**/*.js\"",
    "prepublishOnly": "npm run prettier && npm run build",
    "test": "mocha --recursive --require babel-core/register",
    "test:watch": "mocha --watch --recursive --require babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enzoferey/url-to-deep-link.git"
  },
  "author": "Enzo Ferey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/enzoferey/url-to-deep-link/issues"
  },
  "homepage": "https://github.com/enzoferey/url-to-deep-link#readme",
  "keywords": [
    "url",
    "app",
    "deep",
    "link",
    "social",
    "media"
  ],
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "mocha": "^8.1.3",
    "prettier": "1.18.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0"
  }
}
